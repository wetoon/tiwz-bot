{
    "compilerOptions": {
      "target": "ESNext",
      "module": "ESNext",
      "moduleResolution": "node",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "outDir": "./dist",
      "declaration": true,
      "types": ["@types/bun"]
    },
    "include": ["src/**/*.ts"],
    "exclude": ["node_modules","dist"]
  }
  